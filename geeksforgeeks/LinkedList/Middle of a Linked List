  public:
    int getMiddle(Node* head) {
        // code here
        
        if(!head) return -1;
        if(!head->next) return head->data;
        
        
        Node* slow = head;
        Node* fast = head->next;
        
        while(slow && fast){
            fast = fast->next;
            if(fast) fast = fast->next;
            slow = slow->next;
        }
        
        return slow->data;
        
    }
