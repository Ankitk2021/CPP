    stack<pair<int,int>> s;
    MinStack() {
        
    }
    
    void push(int val) {
      if(s.empty()){
        s.push({val,val});
      }else{
        int mini = s.top().second;
        s.push({val,min(mini,val)});
      }   
    }
    
    void pop() {
       if(!s.empty()) s.pop(); 
    }
    
    int top() {
        if(s.empty()) return -1;
      return s.top().first;  
    }
    
    int getMin() {
        if(s.empty()) return -1;
      return s.top().second;  
    }
